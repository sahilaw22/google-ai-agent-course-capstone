{
  "version": 2,
  "builds": [
    {
      "src": "api/server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/server.py"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "@google_api_key",
    "AGENT_MODEL": "gemini-2.5-flash-lite",
    "AGENT_TEMPERATURE": "0.7"
  }
}
